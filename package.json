{
  "name": "tdnet-digest",
  "version": "0.1.0",
  "private": true,
  "description": "Chrome extension for summarizing TDnet disclosure documents with LLM",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "type-check": "turbo run type-check"
  },
  "devDependencies": {
    "prettier": "^3.4.2",
    "turbo": "^2.3.3"
  },
  "packageManager": "npm@10.9.2",
  "engines": {
    "node": ">=18.0.0"
  }
}
